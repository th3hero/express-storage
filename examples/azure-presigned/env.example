# Azure Blob Storage Presigned URL (SAS) Configuration
FILE_DRIVER=azure-presigned
BUCKET_NAME=

# Option 1: Connection string (recommended - get from Azure Portal → Storage Account → Access Keys)
AZURE_CONNECTION_STRING=

# Option 2: Account credentials (alternative to connection string)
# AZURE_ACCOUNT_NAME=
# AZURE_ACCOUNT_KEY=

# Optional: Container name (uses BUCKET_NAME if not set)
# AZURE_CONTAINER_NAME=

PRESIGNED_URL_EXPIRY=600
